&.map
  position absolute
  top 0px
  left 0px
  bottom 0px
  width 100%
  .demo-popup
    font-family Roboto, sans-serif
    width 268px
    ul
      list-style none
      margin 0px
      padding 0px
      height 170px
      li
        margin 0px
        padding 0px 8px
        height @height
        position relative
        float left
        width 56px
        &.nan
          visibility hidden
        .year
          position absolute
          bottom 0px
          left 0px
          display block
          width 100%
          text-align center
          font-weight 500
        .count
          position absolute
          left 100%
          width 100%
          left 0px
          display block
          text-align center
          font-weight 700
        svg
          position absolute
          bottom 30px
          left 8px
          line
            stroke-width 50
            stroke ColorRed
            stroke-dasharray 1, 1
            shape-rendering crispEdges
